
<h2>Hey <%= current_user.email %>!</h2>

<h3>Here is your Scrutinize list:</h3>
<div class="row">
  <div class="col-md-8">
    <div class="media">
      <div class="media-body">
        <table class="table table-hover">
          <tbody>
            <tr class>
              <% @registered_apps.each do |app| %>
                <%= render 'registered_applications/registered_app', registered_application: app %>
              <% end %>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<h3>Create registered apps:</h3>
<div class="row">
  <div class="col-md-8">
    <div class="media">
      <div class="media-body">
        <table class="table table-hover">
          <tbody>
            <tr>
              <%= render 'registered_applications/form', registered_app: RegisteredApplication.new, user: @user %>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
