<h1><%= @registered_app.name) %></h1>

<div class="row">
  <div class="col-md-8">
    <p><%= @registered_app.url) %></p>
  </div>
  <div class="col-md-4">
    <%= link_to "Edit", edit_user_registered_application(@registered_app), class: 'btn btn-success' %>
    <%= link_to "Delete App", @registered_app, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this app?'} %>
  </div>
</div>
